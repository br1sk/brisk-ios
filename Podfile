# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'Brisk iOS' do
  use_frameworks!
  inhibit_all_warnings!
  
  pod 'InterfaceBacked'
  pod 'Sonar', :git => 'https://github.com/br1sk/Sonar'
  pod 'SVProgressHUD'
  pod 'AcknowList'
  pod 'SwiftLint', '~> 0.25.1'
 
  target 'Brisk iOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
